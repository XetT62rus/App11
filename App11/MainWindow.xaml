<Window x:Class="App11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:App11"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="200">
    <StackPanel>
        <Path Fill="Red" Stroke="Black" >
            <Path.Data>
                <CombinedGeometry>
            <CombinedGeometry.Geometry1>
                        <GeometryGroup>
                <EllipseGeometry RadiusX="50" RadiusY="50" Center="100,100"/>
                    <EllipseGeometry RadiusX="40" RadiusY="40" Center="100,100"/>
                        </GeometryGroup>
                    </CombinedGeometry.Geometry1>                
                <CombinedGeometry.Geometry2>
                    <RectangleGeometry Rect="125,-75,90,10">
                        <RectangleGeometry.Transform>
                            <RotateTransform Angle="135" CenterX="100" CenterY="0"/>
                        </RectangleGeometry.Transform>
                    </RectangleGeometry>
                </CombinedGeometry.Geometry2>
                </CombinedGeometry>
            </Path.Data>
        </Path>
    </StackPanel>
</Window>
